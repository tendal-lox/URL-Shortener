<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>URL shrinker</h1>
    <form action="/shrinkurl" method="post">
        <label for="fullUrl">URL</label>
        <input type="url" name="fullUrl", id="fullUrl">
        <button type="submit">Shrink URL</button>
    </form>
    <table>
        <thead>
            <tr>
                <th>Full URL</th>
                <th>Short URL</th>
            </tr>
        </thead>
        <tbody>
            <% allShrinkedUrl.forEach(shrinkedUrl => { %>
                <tr>
                    <td><a href="<%= shrinkedUrl.fullUrl %>"><%= shrinkedUrl.fullUrl %></a></td>
                    <td><a href="<%= shrinkedUrl.shrinkUrl %>"><%= shrinkedUrl.shrinkUrl %></a></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>